{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^generated/prisma/client$": "<rootDir>/../node_modules/@prisma/client"
  }
  // "transformIgnorePatterns": ["/node_modules/(?!@prisma/client/)"]
}
